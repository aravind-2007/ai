# 18_dfs.py
def dfs(graph, start, visited=None, order=None):
    if visited is None: visited=set()
    if order is None: order=[]
    visited.add(start)
    order.append(start)
    for v in graph.get(start,[]):
        if v not in visited:
            dfs(graph, v, visited, order)
    return order

if __name__ == "__main__":
    g = {
        'A':['B','C'],
        'B':['D','E'],
        'C':['F'],
        'D':[], 'E':[], 'F':[]
    }
    print(dfs(g,'A'))
